#!/bin/sh
# See /usr/share/X11/xkb/rules/base.lst for xkb-options
gsettings set org.gnome.desktop.input-sources sources "[('xkb', 'us+altgr-intl'), ('xkb', 'se')]"
gsettings set org.gnome.desktop.input-sources xkb-options "['nodeadkeys', 'lv3:ralt_switch', 'ctrl:nocaps', 'grp:shifts_toggle']"

gsettings set org.gnome.desktop.background color-shading-type 'solid'
gsettings set org.gnome.desktop.background picture-options 'zoom'
gsettings set org.gnome.desktop.background picture-uri "$HOME/Pictures/Backgrounds/sunset.jpg"
gsettings set org.gnome.desktop.background picture-uri-dark "$HOME/Pictures/Backgrounds/sunset.jpg"
gsettings set org.gnome.desktop.background primary-color '#1e629b'
gsettings set org.gnome.desktop.background secondary-color '#1e629b'

gsettings set org.gnome.desktop.interface clock-show-date true
